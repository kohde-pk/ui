{
  "name": "cards",
  "version": "1.0.0",
  "description": "Portfolio site for Kathleen Ohde",
  "main": "index.html",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss scss/tyle.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "compile:css": "node-sass sass/main.scss css/style.css -w",
    "compile:stylecss": "node-sass sass/cards.scss css/main.css -w",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Kathleen Ohde",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.22",
    "express": "^4.16.4",
    "hacker-news-api": "^2.0.0",
    "popper.js": "^1.14.7"
  }
}
